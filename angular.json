{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "flipper-plugins": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/flipper-plugins",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "flipper-plugins:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "flipper-plugins:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "flipper-plugins:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "flipper-plugins:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "flipper-plugins:serve:production"
            }
          }
        }
      }
    },
    "pharmacy-pos": {
      "projectType": "library",
      "root": "projects/pharmacy-pos",
      "sourceRoot": "projects/pharmacy-pos/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/pharmacy-pos/tsconfig.lib.json",
            "project": "projects/pharmacy-pos/ng-package.json"
          },
          "scripts":[ "projects/pharmacy-pos/node_modules/alasql/dist/alasql.min.js"]
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pharmacy-pos/src/test.ts",
            "tsConfig": "projects/pharmacy-pos/tsconfig.spec.json",
            "karmaConfig": "projects/pharmacy-pos/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/pharmacy-pos/tsconfig.lib.json",
              "projects/pharmacy-pos/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "NDrop": {
      "root": "projects/ndrop",
      "sourceRoot": "projects/ndrop/src",
      "projectType": "library",
      "prefix": "N",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ndrop/tsconfig.lib.json",
            "project": "projects/ndrop/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ndrop/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ndrop/src/test.ts",
            "tsConfig": "projects/ndrop/tsconfig.spec.json",
            "karmaConfig": "projects/ndrop/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ndrop/tsconfig.lib.json",
              "projects/ndrop/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "flipper-pos": {
      "projectType": "library",
      "root": "projects/flipper-pos",
      "sourceRoot": "projects/flipper-pos/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/flipper-pos/tsconfig.lib.json",
            "project": "projects/flipper-pos/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/yegobox-login/src/test.ts",
            "tsConfig": "projects/yegobox-login/tsconfig.spec.json",
            "karmaConfig": "projects/yegobox-login/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/yegobox-login/tsconfig.lib.json",
              "projects/yegobox-login/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngOfflineDb": {
      "projectType": "library",
      "root": "projects/ng-offline-db",
      "sourceRoot": "projects/ng-offline-db/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-offline-db/tsconfig.lib.json",
            "project": "projects/ng-offline-db/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-offline-db/src/test.ts",
            "tsConfig": "projects/ng-offline-db/tsconfig.spec.json",
            "karmaConfig": "projects/ng-offline-db/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-offline-db/tsconfig.lib.json",
              "projects/ng-offline-db/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "flipperMenu": {
      "projectType": "library",
      "root": "projects/flipper-menu",
      "sourceRoot": "projects/flipper-menu/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/flipper-menu/tsconfig.lib.json",
            "project": "projects/flipper-menu/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/flipper-menu/src/test.ts",
            "tsConfig": "projects/flipper-menu/tsconfig.spec.json",
            "karmaConfig": "projects/flipper-menu/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/flipper-menu/tsconfig.lib.json",
              "projects/flipper-menu/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "flipper-plugins"
}